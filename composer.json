{
    "name": "Odalisk",
    "description": "Open DAta LIve Score checKing ",
    "repositories": {
        "knp-unofficial": {
            "type": "package",
            "package": {
                "name": "odalisk/knpradbundle",
                "version": "1.0",
                "source": {
                    "url": "git://github.com/odalisk/KnpRadBundle.git",
                    "type": "git",
                    "reference": "origin/master"
                },
                "autoload": {
                    "psr-0": { "Knp\\Bundle\\RadBundle": "" }
                },
                "target-dir": "Knp/Bundle/RadBundle",
                "bin": ["bin/generate-rad-project"]
            }
        }
    },
    "require": {
        "php":  ">=5.3.2",
        "symfony/symfony":  "2.1.*",
        "twig/extensions":  "*",

        "symfony/assetic-bundle":  "2.0.*",
        "sensio/generator-bundle": "2.0.*",
        "odalisk/knpradbundle": "1.0",
		"sensio/buzz-bundle": "dev-master",
		"doctrine/orm": "2.1.x-dev",
		"stof/doctrine-extensions-bundle": "dev-master"
    }
}